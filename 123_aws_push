import boto3
import sys
import os

#filename = sys.argv[1]

with open("/home/fundwave/flan/shared/ips.txt") as f:
	content = f.readline()
	s3_client = boto3.client('s3')
	try:
    		response = s3_client.upload_file(f"/home/fundwave/flan/shared/reports/{content}.html", "mywavebucket", f"{content}.html")
	except Exception as e:
    		print('Error uploading to s3')
    		print(e)
